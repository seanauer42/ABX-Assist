{
	"name": "ABX Assist",
	"version": "0.1",
	"background": {
		"scripts": ["background.js"],
		"persistant": false
	},	
	"permissions": [
		"activeTab",
		"tabs",
		"storage"
	],
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/abx-air-logo.png",
			"32": "images/abx-air-logo.png",
			"48": "images/abx-air-logo.png",
			"128": "images/abx-air-logo.png"
		}
	},
	"icons": {
		"16": "images/abx-air-logo.png",
		"32": "images/abx-air-logo.png",
		"48": "images/abx-air-logo.png",
		"128": "images/abx-air-logo.png"
	},
	"manifest_version": 2
}
